[mysqld]
default-storage-engine=MyISAM
skip-innodb
key_buffer_size = 16K
max_allowed_packet = 1M
table_open_cache = 4
sort_buffer_size = 64K
read_buffer_size = 256K
read_rnd_buffer_size = 256K
net_buffer_length = 2K
thread_stack = 64K

[isamchk]
key_buffer_size = 8M
sort_buffer_size = 8M

[myisamchk]
key_buffer_size = 8M
sort_buffer_size = 8M
